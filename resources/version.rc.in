
#include <windows.h>
#include "version.h"

IDI_ICON1 ICON "..\\dartomat\\resources\\img\\dartomato.ico"

VS_VERSION_INFO VERSIONINFO
	FILEVERSION @VERSION_MAJOR@,@VERSION_MINOR@,@VERSION_PATCH@
	PRODUCTVERSION @VERSION_MAJOR@,@VERSION_MINOR@,@VERSION_PATCH@
	FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
	FILEFLAGS VS_FF_DEBUG
#else
	FILEFLAGS 0x0L
#endif
	FILEOS VOS_NT_WINDOWS32
	FILETYPE VFT_APP 
	FILESUBTYPE VFT2_UNKNOWN
	BEGIN
		BLOCK "StringFileInfo"
		BEGIN
			BLOCK "040904b0"
			BEGIN
				VALUE "CompanyName", ""
				VALUE "FileDescription", "Dartomat"
				VALUE "FileVersion", "@VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_PATCH@.@COMMIT_HASH@\0"
				VALUE "LegalCopyright", ""
				VALUE "OriginalFilename", "Dartomat.exe"
				VALUE "ProductName", "Dartomat"
				VALUE "ProductVersion", "@VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_PATCH@.@COMMIT_HASH@\0"
				VALUE "InternalName", "Dartomat"
				VALUE "Comments", ""
				VALUE "LegalTrademarks", ""
			END
		END
		BLOCK "VarFileInfo"
		BEGIN
			VALUE "Translation", 0x0409, 1200
	    END
	END